{
    "extends": [
        "@solarwinds/ts-lint-rules/tslint.angular.json"
    ],
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "rd",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "rd",
            "kebab-case"
        ],
        "import-blacklist": [
            true,
            "lodash",
            true,
            "moment"
        ]
    }
}
