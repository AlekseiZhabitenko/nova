{
    "xliffmergeOptions": {
        "srcDir": "src/locale",
        "genDir": "dist",
        "i18nFile": "messages.fr.xlf",
        "i18nFormat": "xlf",
        "encoding": "UTF-8",
        "defaultLanguage": "en-us",
        "languages": ["fr"],
        "removeUnusedIds": false,
        "useSourceAsTarget": false,
        "beautifyOutput": true,
        "verbose": true,
        "quiet": false
    }
}
